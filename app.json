{
  "expo": {
    "name": "teethsi",
    "slug": "veneera_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/Dark.png",
    "scheme": "teethsi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.teethsi.indrajeet",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.indrajeet_dev.teethsi"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icons/Dark.png"
    },
    "plugins": [
      "expo-router",  
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b8f50da3-3cec-4fd0-ac14-8e50b891c28b"
      }
    },
    "owner": "indrajeet_dev"
  }
}
